package university;

public class UniversityException extends Exception {

	int studentID;
	int courseID;
	public UniversityException(String messaggioDescrittivo) {
		super(messaggioDescrittivo);
	}

	public UniversityException(String messaggioDescrittivo, int id) {
		super(messaggioDescrittivo);
		studentID = id;
	}
}
