package managingProperties;

public class Appartment {
	private Building building;
	private int numApp;
	
	public Appartment(Building building, int numApp) {
		super();
		this.building = building;
		this.numApp = numApp;
	}

	/**
	 * @return the building
	 */
	public Building getBuilding() {
		return building;
	}

	/**
	 * @return the numApp
	 */
	public int getNumApp() {
		return numApp;
	}
	
}
